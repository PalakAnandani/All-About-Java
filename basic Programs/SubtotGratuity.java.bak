import java.util.Scanner;
class SubtotGratuity{
	public static void main(String [] args)
	{
		Scanner sc= new Scanner(System.in);
		System.out.println("Enter the subtotal and a tip rate:");
		float subTot= sc.nextFloat();
		float tip=sc.nextFloat();
		float total= subTot*(tip/100);
		System.out.println("the tip is" +tip+" and total is " +total);
	}
}


			

